{
    "name": "H2O web server / SSL proxy",
    "version": "1.1",
    "slug": "h2o",
    "url": "https://gitlab.com/mailaender.it/home-assistant/h2o",
    "description": "A modern high-performance web server",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "startup": "application",
    "boot": "auto",
    "ports": {
        "80/tcp": null,
        "443/tcp": 443
    },
    "map": [
        "config",
	"share",
        "ssl:rw"
    ],
    "options": {
        "domain": "example.com",
        "certificate": "/ssl/fullchain.pem",
        "private_key": "/ssl/privkey.pem",
        "dhparams": "/ssl/dhparams.pem",
        "hsts": "max-age=15552000; includeSubDomains; preload",
        "custom_config": {
            "enabled": false,
            "file": "/config/h2o.conf"
        }
    },
    "schema": {
        "domain": "str",
        "certificate": "str",
        "private_key": "str",
        "dhparams": "str",
        "hsts": "str",
        "custom_config": {
            "enabled": "bool",
            "file": "str"
        }
    }
}
